    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch local web server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch local indexer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/indexer/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch current file",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}/${fileBasenameNoExtension}.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0,
            "cwd": "${workspaceFolder}"
        }
    ]
}